{
	"id":"com.moimz.imodule.module.banner",
	"icon":"xi xi-coupon",
	"title":{
		"ko":"배너"
	},
	"version":"3.0.0",
	"description":{
		"ko":"배너노출과 관련된 전반적인 기능을 관리합니다."
	},
	"author":{
		"name":"Arzz",
		"email":"arzz@arzz.com"
	},
	"homepage":"https://www.imodule.kr",
	"versionChecker":"http://www.moimz.com/ko/api/moimz/getLatest",
	"dependencies":{
		"core":"3.0.0",
		"attachment":"3.0.0"
	},
	"language":"ko",
	"admin":true,
	"context":false,
	"global":false,
	"article":false,
	"widget":true,
	"databases":{
		"banner_group_table":{
			"columns":{
				"idx":{
					"type":"int",
					"length":11
				},
				"title":{
					"type":"varchar",
					"length":50
				},
				"type":{
					"type":"enum",
					"length":"'TEXT','IMAGE','IMAGETEXT'"
				}
			},
			"indexes":{
				"idx":"primary_key"
			},
			"auto_increment":"idx"
		},
		"banner_table":{
			"columns":{
				"idx":{
					"type":"int",
					"length":11
				},
				"gidx":{
					"type":"int",
					"length":11
				},
				"url":{
					"type":"varchar",
					"length":255
				},
				"target":{
					"type":"enum",
					"length":"'_blank','_self','_top'",
					"default":"_blank"
				},
				"text":{
					"type":"varchar",
					"length":255,
					"is_null":true
				},
				"permission":{
					"type":"varchar",
					"length":255
				},
				"sort":{
					"type":"int",
					"length":11,
					"default":0
				},
				"reg_date":{
					"type":"int",
					"length":11
				}
			},
			"indexes":{
				"idx":"primary_key",
				"gidx":"index"
			},
			"auto_increment":"idx"
		}
	}
}