{
	"id":"com.moimz.imodule.module.banner",
	"icon":"xi xi-coupon",
	"title":{
		"ko":"배너"
	},
	"version":"3.0.0",
	"description":{
		"ko":"배너노출과 관련된 전반적인 기능을 관리합니다."
	},
	"author":{
		"name":"Arzz",
		"email":"arzz@arzz.com"
	},
	"homepage":"https://www.imodule.kr",
	"versionChecker":"http://www.moimz.com/ko/api/moimz/getLatest",
	"dependencies":{
		"core":"3.0.0",
		"attachment":"3.0.0"
	},
	"language":"ko",
	"admin":true,
	"context":false,
	"global":false,
	"article":false,
	"widget":true,
	"databases":{
		"banner_group_table":{
			"columns":{
				"idx":{
					"type":"int",
					"length":11,
					"comment":"고유값"
				},
				"title":{
					"type":"varchar",
					"length":50,
					"comment":"그룹관리"
				},
				"type":{
					"type":"enum",
					"length":"'TEXT','TITLETEXT','IMAGE','IMAGETEXT'",
					"comment":"종류"
				}
			},
			"indexes":{
				"idx":"primary_key"
			},
			"auto_increment":"idx",
			"comment":"배너그룹관리"
		},
		"banner_table":{
			"columns":{
				"idx":{
					"type":"int",
					"length":11,
					"comment":"고유값"
				},
				"gidx":{
					"type":"int",
					"length":11,
					"comment":"그룹코드"
				},
				"url":{
					"type":"varchar",
					"length":255,
					"comment":"URL LINK"
				},
				"target":{
					"type":"enum",
					"length":"'_blank','_self','_top'",
					"default":"_blank",
					"comment":"새창여부"
				},
				"title":{
					"type":"varchar",
					"length":255,
					"is_null":true,
					"comment":"배너명"
				},
				"text":{
					"type":"varchar",
					"length":255,
					"is_null":true,
					"comment":"배너내용"
				},
				"permission":{
					"type":"varchar",
					"length":255,
					"comment":"사용여부"
				},
				"sort":{
					"type":"int",
					"length":11,
					"default":0,
					"comment":"순서"
				},
				"reg_date":{
					"type":"int",
					"length":11,
					"comment":"등록일"
				}
			},
			"indexes":{
				"idx":"primary_key",
				"gidx":"index"
			},
			"auto_increment":"idx",
			"comment":"배너관리"
		}
	}
}